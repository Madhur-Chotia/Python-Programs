   "source": [
    "x=input(\"Enter the number for which you want the table\")\n",
    "table_x=lambda t: int(x) * t\n",
    "\n",
    "for i in range(1,11):\n",
    "    res=table_x(i)\n",
    "    print(f\"{x} X {i} = {res}\")"
   ]
  }
